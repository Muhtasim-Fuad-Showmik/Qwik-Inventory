{
  "name": "my-qwik-basic-starter",
  "description": "App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint src/**/*.ts*",
    "preview": "qwik build preview && vite preview --open",
    "serve": "echo 'Run \"npm run qwik add\" to install a server adaptor'",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.13.3",
    "@builder.io/qwik-city": "0.0.122",
    "@builder.io/qwik-react": "0.1.1",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mui/material": "5.10.10",
    "@mui/x-data-grid": "5.17.8",
    "@types/eslint": "8.4.9",
    "@types/node": "latest",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "eslint": "8.26.0",
    "eslint-plugin-qwik": "0.13.3",
    "node-fetch": "3.2.10",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.8.4",
    "vite": "3.2.2",
    "vite-tsconfig-paths": "3.5.0"
  },
  "dependencies": {
    "@mui/x-date-pickers": "^5.0.10",
    "chart.js": "^4.0.1",
    "dayjs": "^1.11.7",
    "react-chartjs-2": "^5.0.1"
  }
}
